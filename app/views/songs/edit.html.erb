  
<h1>Update Song</h1>

<%= form_for(@song) do |f| %>
  <label>Song Name:</label><br>
  <%= f.text_field :name, placeholder: @song.name%><br>
 
  <label>Genre:</label><br>
  <%= f.number_field :genre_id, placeholder: @my_genre.id %>
  

  <label>Artist:</label><br>
  <%= f.number_field :artist_id, placeholder: @my_artist.id %>
  

  
  <%= f.submit %>
<% end %>