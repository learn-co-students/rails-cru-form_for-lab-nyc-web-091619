<h1>Update Song</h1>

<%= form_for @song do |f| %>
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %>
    <%= f.label :artist_id, "Artist" %>
    <%= f.text_field :artist_id %>
    <%= f.label :genre_id, "genre" %>
    <%= f.text_field :genre_id %>
    <%= f.submit "Update Song"%>
<% end %>