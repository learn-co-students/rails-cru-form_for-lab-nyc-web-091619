<h1>New Song Form</h1>
<%= form_for @song do |f| %>
    <%= f.label :name, "Name" %>
    <%= f.text_field :name %><br>
    <%= f.label :artist_id, "Artist" %>
    <%= f.text_field :artist_id %>
    <%= f.label :genre_id, "Genre" %>
    <%= f.text_field :genre_id %>
    <%= f.submit "Create Song" %>
<% end %>
